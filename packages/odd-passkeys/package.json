{
  "name": "@oddjs/passkeys",
  "version": "0.1.0",
  "description": "ODD SDK Passkeys components.",
  "author": "Hugo Dias <hugomrdias@gmail.com> (hugodias.me)",
  "license": "apache-2.0",
  "repository": {
    "url": "oddsdk/passkeys",
    "directory": "packages/odd-passkeys"
  },
  "homepage": "https://github.com/oddsdk/passkeys/tree/main/packages/odd-passkeys",
  "keywords": [
    "webauthn",
    "passkeys",
    "authentication",
    "fido",
    "odd",
    "odd-sdk"
  ],
  "type": "module",
  "main": "src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./src/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "dist/src/index"
      ]
    }
  },
  "files": [
    "src",
    "dist/src/*.d.ts",
    "dist/src/*.d.ts.map"
  ],
  "scripts": {
    "lint": "tsc --build && eslint '**/*.{js,ts}' && prettier --check '**/*.{js,ts,yml,json}' --ignore-path ../../.gitignore",
    "test": "tsc --build && pnpm run test:node && pnpm run test:browser",
    "test:node": "mocha 'test/**/!(*.browser).test.js'",
    "test:browser": "playwright-test 'test/**/!(*.node).test.js'"
  },
  "dependencies": {
    "@noble/ed25519": "^2.0.0",
    "@oddjs/odd": "^0.37.1",
    "iso-base": "^0.1.5",
    "iso-passkeys": "^0.1.5"
  },
  "devDependencies": {
    "@types/assert": "^1.5.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.10",
    "assert": "^2.0.0",
    "hd-scripts": "^6.0.0",
    "mocha": "^10.2.0",
    "playwright-test": "^9.1.0",
    "typescript": "5.0.4"
  },
  "eslintConfig": {
    "extends": [
      "../../node_modules/hd-scripts/eslint/index.js"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "env": {
      "mocha": true
    },
    "ignorePatterns": [
      "dist"
    ]
  },
  "depcheck": {
    "specials": [
      "bin"
    ],
    "ignores": [
      "@types/*",
      "hd-scripts",
      "assert"
    ]
  }
}
